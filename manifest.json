{
  "name": "Prep Prompt ChatGPT",
  "version": "1.0",
  "description": "Automatically requests that ChatGPT responds in French.",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "action": {
    "default_title": "Prep Prompt ChatGPT",
    "default_popup": "settings.html"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["populate-text-field.js"]
    }
  ]
}
